{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/((?!api|uploads|_expo|assets|favicon.ico).*)",
      "destination": "/index.html"
    }
  ]
}